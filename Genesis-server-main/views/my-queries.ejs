
<head>
    <link rel="stylesheet" href="/css/my-queries.css">
</head>

<%- include('navbar.ejs') %>
<!--<div class="wrap">-->
<div class="heading-wrap"><p class="heading">Your Queries</p></div>

<div class="container2">

    <% for(let i=0;i<arr.length;i++){%>
        <div class="items2">

                <div class="inner-item Q">
                 Q  <%=i+1%>). <%=arr[i].question%>
                </div>
            <div style="display: flex;width: 100%;justify-content: space-evenly;padding-top: 7px">
                <p style="font-size: 14px;color: #3e54ac;">Asked on:<%=arr[i].askDate%></p>
                <p style="font-size: 14px;color: #3e54ac;">Answered on:<%=arr[i].answerDate%></p>

            </div>

            <div class="inner-item A">
                    <%=arr[i].answer%>
                </div>


        </div>


    <%}%>

</div>

<%-include('footer.ejs')%>
<!--</div>-->

